{
  "name": "angular-starter",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ssr": "ng run angular-starter:serve-ssr",
    "build": "ng build",
    "build:stats": "ng build --stats-json",
    "build:prod": "ng build --prod",
    "build:ssr": "ng build --prod && ng run angular-starter:server:production",
    "serve:stats": "webpack-bundle-analyzer dist/angular-starter/browser/stats-es2015.json",
    "serve:prod": "http-server -p 8000 -c-1 dist/angular-starter/browser --proxy http://localhost:8000?",
    "serve:ssr": "node dist/angular-starter/server/main.js",
    "test": "ng test",
    "test:without-watch": "ng test --watch false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ng run angular-starter:prerender",
    "analyze": "npm run build:stats && npm run serve:stats",
    "format": "prettier --write \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:watch": "onchange \"**/*.{ts,js,css,scss,html,json,md}\" -- prettier --write {{changed}}",
    "format:check": "prettier --check \"**/*.{ts,js,css,scss,html,json,md}\"",
    "format:staged": "pretty-quick --staged --pattern \"**/*.{ts,js,css,scss,html,json,md}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/platform-server": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@angular/service-worker": "~10.0.3",
    "@avivharuzi/ngx-seo": "^1.0.1",
    "@ngrx/effects": "^9.2.0",
    "@ngrx/store": "^9.2.0",
    "@ngrx/store-devtools": "^9.2.0",
    "@nguniversal/express-engine": "^10.0.1",
    "express": "^4.15.2",
    "material-design-icons": "^3.0.1",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.2",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@nguniversal/builders": "^10.0.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.5",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
